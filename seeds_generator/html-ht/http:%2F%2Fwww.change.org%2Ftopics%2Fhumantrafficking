!function () {
  var DOCUMENT = $(document);

  DOCUMENT.on({
    submit: function () {
      var form = $(this);
      var originalSearchValue = form.find('input').val();
      Change.hash('search/' + originalSearchValue);
    },

    searched: function () {
      Change.Form(this).notSubmitting();
    }
  }, "form.search");

  DOCUMENT.on('petition-tab-change', function () {
    // display search layout
    var params = $(this);
    if (params.view == 'search'){
      $('#body').addClass('searching');
      fillSearchBoxes(params.search);
    }else{
      $('#body').removeClass('searching');
    }
  });

  function fillSearchBoxes(value){
    $('form.search').find('input[type=text]').each(function(index){
      $(this).val(value);
    });
  }
}();

