    <script>
    require(["jquery"], function($){
        $.ajax({
            url : '/m/241708400/jforum_preview/'
        }).success( function(response, status) {        	
            if (response) {
            	$(document).ready(function() {
                    $('#threadsPreview').html(response);
                });
