<script type="text/javascript">
    require(["jquery","globals", "domReady!"], function ($, RT) {
        $("#fbLoginButton").click(function() {
            RT.fb_ready.done(function() {
                FB.login(function(response) {
                }, {scope: "email,publish_actions"});
            });
        });
        // if the user is signed in with facebook, just hide the login modals
        // right away just in case they started the flow
        RT.fb_connected.done(function onFbLoggedIn() {
            $("#signup").modal('hide');
            $("#login").modal('hide');
        });
    })();
</script>

