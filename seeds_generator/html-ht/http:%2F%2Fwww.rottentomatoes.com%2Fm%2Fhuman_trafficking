    require(["jquery", "domReady!"], function($) {
        $("#audience_reviews .quote_bubble").each(function() {
            if ($(".comment", this).height() < 158) {
                $("a.more", this).remove();
            }
        });
        $("#audience_reviews .quote_bubble a.more").click(function() {
            $(this).siblings(".comment").addClass("full");
            $(this).remove();
        });
        $(".ratingButton").click(function() {
            $("html, body").animate({
                scrollTop: $("#rating_widget").offset().top - $(window).height()/2
            }, 1000);
        });
    });
</script>

        










































































